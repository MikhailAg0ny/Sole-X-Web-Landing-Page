{
  "name": "web_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "optimize:model": "gltf-transform optimize ./public/models/nike_air_zoom_pegasus_36.glb ./public/models/nike_air_zoom_pegasus_36.optim.glb --texture-size 1024 --texture-compress webp",
    "optimize:model:jordan": "gltf-transform optimize ./public/models/air_jordan_1_1985.glb ./public/models/air_jordan_1_1985.optim.glb --texture-size 1024 --texture-compress webp",
    "optimize:model:jordan:512": "gltf-transform optimize ./public/models/air_jordan_1_1985.glb ./public/models/air_jordan_1_1985.optim.glb --texture-size 512 --texture-compress webp",
    "optimize:model:jordan2015:512": "gltf-transform optimize ./public/models/2015_jordan_1_high_chicago_gs.glb ./public/models/2015_jordan_1_high_chicago_gs.optim.glb --texture-size 512 --texture-compress webp --meshopt false",
    "optimize:model:jordan2015:512:nomesh": "gltf-transform transform ./public/models/2015_jordan_1_high_chicago_gs.glb ./public/models/2015_jordan_1_high_chicago_gs.optim.glb -t dedup -t instance -t palette -t flatten -t join -t weld -t simplify -t resample -t prune -t sparse -t textureCompress --texture-size 512 --texture-compress webp",
    "optimize:model:shoe": "gltf-transform optimize ./public/models/shoe.glb ./public/models/shoe.optim.glb --texture-size 1024 --texture-compress webp"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.25.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mantine/core": "^8.2.7",
    "@mantine/hooks": "^8.2.7",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.2",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@gltf-transform/cli": "^4.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2"
  }
}
